---
layout: nil
---
[
	{% for p in site.posts %}
		{
			"content": "{{ p.content | strip_newlines }}",
			"date": "{{ p.date }}",
			"excerpt": "{{ p.content | split:"<!-- more -->" | first | strip_newlines }}",
			"title": "{{ p.title }}"
		}
		{% unless forloop.last %},{% endunless %}
	{% endfor %}
]
